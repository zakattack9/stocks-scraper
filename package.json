{
  "name": "stockup-server",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon testserver.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node testserver.js",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "axios": "latest",
    "body-parser": "^1.18.3",
    "captcha-solver": "0.0.6",
    "cheerio": "latest",
    "cloudscraper": "^4.1.2",
    "express": "latest",
    "path": "^0.12.7",
    "puppeteer": "^1.18.1",
    "random-useragent": "^0.3.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.1"
  },
  "description": "A stock application utilizing APIs and web scraping for gathering stock news and data",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakattack9/stockup.git"
  },
